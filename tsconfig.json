{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "moduleResolution": "bundler", // Qo‘shildi
    "module": "esnext",            // Qo‘shildi (tavsiya etiladi)
    "jsx": "react-native",         // Qo‘shildi (Expo uchun mos)
    "esModuleInterop": true,       // Qo‘shildi (modul mosligi uchun)
    "skipLibCheck": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
